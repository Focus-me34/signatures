<h1>Form</h1>
<div class="container-sell-form">
    <div class="container-form">
        <h3 class="title-form">Creating company</h3>
        <%= simple_form_for @company do |f| %>
          <%= f.input :title, label_html: { class: "row justify-content-center"} %>
          <%= f.input :first_name, input_html: { class: "row justify-content-center"}, label_html: { class: "row justify-content-center"} %>
          <%= f.input :last_name, label_html: { class: "row justify-content-center"} %>
          <%= f.input :birth_date, label_html: { class: "row justify-content-center"} %>
          <%= f.input :phone_number, label_html: { class: "row justify-content-center"} %>
          <%= f.button :submit, 'Confirm details', class: 'btn-submit' %>
        <% end %>
      </div>
    <div class="container-form-img">
    </div>
</div>
